<%= form_for(@user) do |f| %>
<br><br>
<%= f.label :name%>
<%= f.text_field :name%>
<br><br>
<%= f.label :email%>
<%= f.email_field :email%>
<br><br>
<%= f.label :password%>
<%= f.password_field :password %>
<br><br>
<%= f.label :administrador%>
<%=  f.check_box :admin %>
<br><br>
<%= f.submit %>

<% end %>